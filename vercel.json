{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/search",
        "dest": "/api/search"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }